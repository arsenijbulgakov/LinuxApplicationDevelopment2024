#: guess_number.h:91
#, c-format
msgid "Usage: %s [OPTION]\n"
msgstr "Использование: %s [АРГУМЕНТ]\n"

#: guess_number.h:92
#, c-format
msgid "Options:\n"
msgstr "Аргументы:\n"

#: guess_number.h:93
#, c-format
msgid "    --help: Print help message\n"
msgstr "    --help: Напечатать help сообщение\n"

#: guess_number.h:94
#, c-format
msgid "    -r:     Use roman digit instead of arabic\n"
msgstr "    -r:     Использовать римские цифры вместо арабских\n"

#: guess_number.c:47
#, c-format
msgid "Choose a number from %s to %s\n"
msgstr "Выберете число от %s до %s\n"

#: guess_number.c:49
#, c-format
msgid "Choose a number from 1 to 100\n"
msgstr "Выберете число от 1 до 100\n"

#: guess_number.c:56
#, c-format
msgid "Is the number greater than %s?\n"
msgstr "Ваше число больше чем %s?\n"

#: guess_number.c:58
#, c-format
msgid "Is the number greater than %d?\n"
msgstr "Ваше число больше чем %d?\n"

#: guess_number.c:62
msgid "yes"
msgstr "да"

#: guess_number.c:64
msgid "no"
msgstr "нет"

#: guess_number.c:67
#, c-format
msgid "The answer must be yes or no. Please enter your answer again\n"
msgstr "Ответ должен быть да или нет. Введите ваш ответ еще раз\n"

#: guess_number.c:72
#, c-format
msgid "Your number is: %s\n"
msgstr "Ваше число: %s\n"

#: guess_number.c:74
#, c-format
msgid "Your number is: %d\n"
msgstr "Ваше число: %d\n"
